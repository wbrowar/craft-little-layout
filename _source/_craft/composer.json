{
    "require": {
        "craftcms/cms": "3.6.5.1",
        "vlucas/phpdotenv": "^3.4.0",
        "wbrowar/little-layout": "@dev"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "/var/www/html/.assembly-cache/setup/plugin/",
            "symlink": true
        }
    ]
}

